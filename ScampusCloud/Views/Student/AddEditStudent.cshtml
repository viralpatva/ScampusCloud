@section Scripts{
    <script>
    $(function () {
         @if(TempData.ContainsKey("Message"))
        {
            @:toastr.success('@TempData["Message"]');
        }
    });

     $('.kt_datepicker').datepicker({
             format: 'mm/dd/yyyy',
            minView: 2,
            maxView: 4,
            autoclose: true
        });

     $('.kt_datepicker').datepicker()
            .on('input change', function (e) {
                var dateString = $(this).val();
                var myDate = new Date(dateString);
                var today = new Date();
                if (myDate > today) {
                    $("p").remove(".alertMsg");
                    $('#dtDOB').after('<p class="alertMsg" style="color:red;">Birth Date can not be greater than Today!</p>');
                    return false;
                }
                else {
                    $("p").remove(".alertMsg");
                }
                return true;
            });


        function loadEmpImage(input) {
            var formData = new FormData();
            var File = input.files[0];
            var extnType = input.files[0].type;
            var extn = extnType.split("/");
            formData.append("file", File);

            if (extn[1] == "png" || extn[1] == "jpg" || extn[1] == "jpeg") {

                var url = "@Url.Action("PreviewSelectedImage", "Student")";
                $.ajax({
                    type: "POST",
                    url: url,
                    data: formData,
                    contentType: false,
                    processData: false,
                    error: function (error) {
                    },
                    success: function (data) {
                        $('#dv_EmpImag').css('background-image', 'url()');
                        $('.empimage').css('background-image', 'url(data:image/jpg;base64,' + data + ')');
                        $('#RemoveEmplImg').show();
                        $("#empImgSrc").val(data);
                        $("#hdnEmployeePicModified").val(true);
                        $("#hdnEmployeePicFileType").val(extn[1]);
                    }
                });
            }
            else {
                var $el = $('#externalEmpImgId');
                $el.wrap('<form>').closest('form').get(0).reset();
                $el.unwrap();
                toastr.error("Please select only images!");
            }
        };

        function RemoveEmpImage() {
            var defaultImg='';
            $('#externalEmpSignImgId').val('');
            $('.empimage').css('background-image', 'none');
            $('#empImgSrc').val(defaultImg);
            $('#RemoveEmplImg').hide();
            var imageUrl = 'data:image/png;base64,'+defaultImg+'';
            $('#dv_EmpImag').css('background-image', 'url(' + imageUrl + ')');
        }
        $("#chkIsactive").change(function () {
            $("#hdnIsactive").val($(this).is(':checked'));
        });

        $('#ddlCampus').change(function () {
            $.ajax({
                type: "post",
                url: "/Student/GetColleageByCampus",
                data: { ScampusId: $('#ddlCampus').val() },
                datatype: "json",
                traditional: true,
                success: function (data) {
                    var colleage = "<select id='ddlColleage'>";
                    colleage = colleage + '<option value="">--Select--</option>';
                    for (var i = 0; i < data.length; i++) {
                        colleage = colleage + '<option value=' + data[i].Value + '>' + data[i].Text + '</option>';
                    }
                    colleage = colleage + '</select>';
                    $('#ddlColleage').html(colleage);
                }
            });
        });

        $('#ddlColleage').change(function () {
            $.ajax({
                type: "post",
                url: "/Student/GetDepartmentByColleage",
                data: { ColleageId: $('#ddlColleage').val() },
                datatype: "json",
                traditional: true,
                success: function (data) {
                    var department = "<select id='ddlDepartment'>";
                    department = department + '<option value="">--Select--</option>';
                    for (var i = 0; i < data.length; i++) {
                        department = department + '<option value=' + data[i].Value + '>' + data[i].Text + '</option>';
                    }
                    department = department + '</select>';
                    $('#ddlDepartment').html(department);
                }
            });
        });

        $('#ddlAdminssionType').change(function () {
            $.ajax({
                type: "post",
                url: "/Student/GetAdmissionTypeShort",
                data: { AdmissionTypeId: $('#ddlAdminssionType').val() },
                datatype: "json",
                traditional: true,
                success: function (data) {
                    var admissionTypeShort = "<select id='ddlAdminssionTypeShort'>";
                    admissionTypeShort = admissionTypeShort + '<option value="">--Select--</option>';
                    for (var i = 0; i < data.length; i++) {
                        admissionTypeShort = admissionTypeShort + '<option value=' + data[i].Value + '>' + data[i].Text + '</option>';
                    }
                    admissionTypeShort = admissionTypeShort + '</select>';
                    $('#ddlAdminssionTypeShort').html(admissionTypeShort);
                }
            });
        });
    </script>
}